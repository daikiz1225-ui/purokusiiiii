{
  "version": 2,
  "rewrites": [
    {
      "source": "/bare/(.*)",
      "destination": "/api/index.js"
    },
    {
      "source": "/service/(.*)",
      "destination": "/index.html"
    }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        { "key": "Service-Worker-Allowed", "value": "/" }
      ]
    }
  ]
}
